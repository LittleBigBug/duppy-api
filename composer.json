{
    "repositories":
    [
        {
            "type": "vcs",
            "url": "https://github.com/Littlebigbug/hybridauth"
        }
    ],
    "require": {
        "php": "^8.0",
        "slim/slim": "^4.6",
        "slim/psr7": "^1.0",
        "vlucas/phpdotenv": "^4.1",
        "php-di/php-di": "^6.1",
        "doctrine/orm": "^2.8",
        "ramsey/uuid-doctrine": "^1.6",
        "yosymfony/toml": "^1.0",
        "hybridauth/hybridauth": "dev-master",
        "web-token/jwt-framework": "^2.2",
        "phpmailer/phpmailer": "^6.2",
        "eftec/bladeone": "^3.47",
        "guzzlehttp/guzzle": "^7.2",
        "ext-dom": "*",
        "spatie/enum": "^3.6",
        "geertw/ip-anonymizer": "^1.1",
        "touhonoob/rate-limit": "^2.0",
        "akrabat/ip-address-middleware": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phing/phing": "^2.0",
        "pdepend/pdepend": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Duppy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duppy\\Tests\\": "tests/"
        }
    }
}
